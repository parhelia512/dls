{
  "name": "dcd",
  "description": "The D Completion Daemon is an auto-complete program for the D programming language",
  "copyright": "Copyright Â© 2015-2020, Brian Schott",
  "authors": [
    "Brian Schott"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    ":dsymbol": "*",
    ":libdparse": "*",
    ":common": "*",
    ":emsi_containers": "*"
  },
  "subPackages": ["dsymbol", "libdparse", "common", "containers", "msgpack-d"],
  "versions": ["built_with_dub"],
  "configurations": [
    {
      "name": "library",
      "targetType": "staticLibrary",
      "lflags-ldc2": [ "-static" ],
      "excludedSourceFiles": [
        "src/dcd/client/*",
        "src/dcd/server/main.d"
      ]
    },
    {
      "name": "client",
      "targetType": "executable",
      "targetPath": "bin/",
      "targetName": "dcd-client",
      "excludedSourceFiles": [
        "src/dcd/server/*"
      ]
    },
    {
      "name": "server",
      "targetType": "executable",
      "targetPath": "bin/",
      "targetName": "dcd-server",
      "dflags": ["-static"],
      "excludedSourceFiles": [
        "src/dcd/client/*"
      ]
    }
  ]
}
